.uni-custom .uni-tip {

  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;

  width: 300px;
  background-color: #fff;
  border-radius: 10px;
}
.uni-custom .uni-tip-title {
  margin: 35rpx 0 20rpx;
  text-align: center;
  font-weight: bold;
  font-size: 16px;
  color: #333;
}
.uni-custom .uni-tip-content {
  padding: 30rpx;
