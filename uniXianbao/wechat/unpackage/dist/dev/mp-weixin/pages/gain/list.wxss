.list {
  margin: 0 24rpx;
  background: #FFF;
  border-radius: 30rpx;
  padding: 0 42rpx;
}
.list .list-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1rpx solid #dbdbdb;
}
.list .list-item view {
  font-size: 24rpx;
  line-height: 76rpx;
}
.list .list-item view:first-child {
  width: 220rpx;
  color: #353038;
}
.list .list-item view:last-child {
  width: 110rpx;
  color: #bda475;
}
.list .list-item:last-child {
  border: 0;
}

