/* 提示窗口 */
.uni-custom .uni-tip {

  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;

  width: 300px;
  background-color: #fff;
  border-radius: 10px;
}
.uni-custom .uni-tip-title {
  margin: 35rpx 0 20rpx;
  text-align: center;
  font-weight: bold;
  font-size: 16px;
  color: #333;
}
.uni-custom .uni-tip-content {
  padding: 30rpx;
  text-align: center;
  font-size: 14px;
  color: #666;
  line-height: 1;
}
.uni-custom .uni-tip-group-button {

  display: -webkit-box;
  display: -webkit-flex;
  display: flex;

  margin-top: 20rpx;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.uni-custom .uni-tip-button {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  border-top: 1px solid #ebebeb;
  padding: 20rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #3b4144;
}
.uni-custom .uni-tip-button:first-child {
  border-right: 1px solid #ebebeb;
}
.uni-custom .uni-tip-button:last-child {
  color: #f59a23;
}
.goods-list {
  margin-top: 110rpx;
}
.goods-list .coupon-page {
  background: #fff;
  padding: 10px;
}
.goods-list .coupon-page .image {
  float: left;
  width: 40%;
}
.goods-list .coupon-page .image image {
  width: 260rpx;
  height: 260rpx;
  border-radius: 3px;
}
.goods-list .coupon-page .content {
  float: right;
  width: 60%;
}
.goods-list .coupon-page .content .coupon {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 18rpx 0;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.goods-list .coupon-page .content .coupon .quan {
  border-radius: 6rpx;
  border: 4rpx solid #f59a23;
  color: #999999;
  font-size: 26rpx;
  padding: 0rpx 20rpx;
}
.goods-list .coupon-page .content .title {
  color: #333;
  font-weight: 400;
  font-size: 30rpx;
  text-overflow: ellipsis;
  height: 84rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
}
.goods-list .coupon-page .content .title uni-tag .uni-tag {
  color: #fff;
  background: #f59a23 !important;
  border-color: #f59a23 !important;
}
.goods-list .coupon-page .content .num {
  color: #999999;
  line-height: 20px;
  font-size: 28rpx;
}
.goods-list .coupon-page .content .num .tmprice {
  padding-right: 10px;
  margin-right: 10px;
  position: relative;
  z-index: 1;
  zoom: 1;
  display: inline-block;
}
.goods-list .coupon-page .content .num .volume {
  float: right;
}
.goods-list .coupon-page .content .money {
  font-size: 20px;
  margin-top: 5px;
}
.goods-list .coupon-page .content .money .coupon-price {
  color: #333;
  font-weight: bold;
  font-size: 26rpx;
}
.goods-list .coupon-page .content .money .coupon-price text {
  padding-left: 5px;
}
.goods-list .coupon-page .content .money .mini-btn {
  background: #f59a23;
  padding: 0 10rpx;
  margin-right: 18rpx;
  border-color: #f59a23;
  float: right;
}
.goods-list .coupon-page .content .upgrade-tip.v1, .goods-list .coupon-page .content .upgrade-tip.v2 {
  font-size: 24rpx;
  color: #666;
  font-weight: bold;
  padding: 1px 15px;
  border-radius: 6rpx;
}
.goods-list .coupon-page .content .upgrade-tip.v2 {
  color: #f59a23;
}
.navbar {
  position: fixed;
  left: 0;
  top: 0px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 80rpx;
  background: #fff;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.06);
  z-index: 10;
}
