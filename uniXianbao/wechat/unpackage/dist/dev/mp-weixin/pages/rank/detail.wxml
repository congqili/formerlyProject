<block wx:if="{{isLoad}}"><view class="container"><view class="carousel"><swiper indicator-dots="{{true}}" circular="true" duration="400"><block wx:for="{{goods.image}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="swiper-item"><view class="image-wrapper"><image class="loaded" src="{{item}}" mode="aspectFill"></image></view></swiper-item></block></swiper></view><view class="introduce-section"><view class="title"><uni-tag class="list-tag" vue-id="526dbcbb-1" text="苏宁" type="primary" size="small" bind:__l="__l"></uni-tag>{{''+goods.detail.commodityInfo.commodityName+''}}</view><view class="bot-row"><block wx:if="{{goods.detail.couponInfo.couponUrl===''}}"><text class="tmprice">{{"优惠价 ¥"+goods.detail.commodityInfo.commodityPrice}}</text></block><block wx:else><text class="tmprice">{{"券后价 ¥ "+goods.detail.price.coupon}}</text></block><text>{{"月销量："+$root.m0}}</text><text data-event-opts="{{[['tap',[['toBuy',['$0'],['goods.link']]]]]}}" bindtap="__e">查看详情 ><text class="yticon icon-you"></text></text></view><block wx:if="{{goods.detail.couponInfo.couponUrl!==''}}"><view class="coupon"><text class="quan">{{"优惠券 ¥"+goods.detail.couponInfo.couponValue}}</text></view></block><block wx:else><view class="coupon"><text class="quan none"></text></view></block><text class="tip-text">{{"佣金比例："+goods.detail.price.rate+"%"}}</text></view><view class="c-list"><view class=" clearfix"><view class="col"><uni-tag vue-id="526dbcbb-2" text="复制文字" type="warning" circle="{{true}}" data-event-opts="{{[['^click',[['copyContent',['$0',true],['goods']]]]]}}" bind:click="__e" bind:__l="__l"></uni-tag></view><view class="col"><uni-tag vue-id="526dbcbb-3" text="分享图片" type="error" circle="{{true}}" data-event-opts="{{[['^tap',[['previewImage',['$0'],['goods.image']]]]]}}" bind:tap="__e" bind:__l="__l"></uni-tag></view><block wx:if="{{shopconfig.showVideo!=0&&goods.video!=''}}"><view class="col"><uni-tag vue-id="526dbcbb-4" text="保存视频" type="primary" circle="{{true}}" data-event-opts="{{[['^tap',[['toVideo',['$0','$1'],['goods.video','goods.commodityInfo.commodityName']]]]]}}" bind:tap="__e" bind:__l="__l"></uni-tag></view></block></view><view class=" clearfix"><parser vue-id="526dbcbb-5" html="{{goods.content}}" bind:__l="__l"></parser></view></view><view class="c-list"><view class="c-row clearfix"><text class="tit">商品编号</text><view class="bz-list con"><text>{{goods.detail.commodityInfo.commodityCode}}</text></view></view><view class="c-row clearfix"><text class="tit">推荐理由</text><view class="bz-list con"><text>{{goods.detail.commodityInfo.sellingPoint}}</text></view></view></view><view class=" c-desc"><text class="tit"><text class="iconfont"></text>苏宁商品</text><text class="tit"><text class="iconfont"></text>品质保证</text><text class="tit"><text class="iconfont"></text>无忧售后</text></view></view></block>