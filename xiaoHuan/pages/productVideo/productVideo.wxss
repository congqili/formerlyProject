view,
text {
  font-family: -apple-system, BlinkMacSystemFont, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Segoe UI", "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.blue-green-btn {
  background: linear-gradient(to right, #38debf, #12a5bf);
}
.blue-purple-btn {
  background: linear-gradient(to right, #4d8be1, #889cf4);
}
view,
text {
  line-height: 1;
}
button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: 1;
}
button {
  border-radius: 0;
}
.banxin {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
}
.col:after {
  content: "";
  height: 0;
  clear: both;
  overflow: hidden;
  display: block;
  visibility: hidden;
}
.share-pop {
  position: fixed;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
}
.share-pop .share-body {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  top: calc(5vh);
  top: calc(5vh + env(safe-area-inset-bottom));
}
.share-pop .share-body .close {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.share-pop .share-body .close image {
  width: 50rpx;
  height: 50rpx;
}
.share-pop .share-body image {
  width: 80vw;
  height: 77vh;
  border-radius: 20rpx;
}
.share-pop .share-body button {
  position: relative;
  width: 80vw;
  height: 5vh !important;
  font-size: 25rpx;
  color: #FFF;
  line-height: 5vh;
  border-radius: 5rpx;
  background: linear-gradient(to right, #39dfbf, #13a6bf);
  text-align: center;
  margin-top: 3vh;
  /*margin-top: 40rpx;*/
}
.share-pop .share-body .share-btn {
  margin-top: 2vh;
}
.com-product-item:first-child {
  border-top: 0!important;
}
.com-product-item {
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-top: 1rpx dotted #dcdcdc;
}
.com-product-item .subsidy {
  top: 0;
  height: 34rpx;
  white-space: nowrap;
  border-radius: 17rpx;
  padding: 0 10rpx;
  color: #957052;
  font-size: 18rpx;
  background: #fcf1d8;
  position: absolute;
  line-height: 34rpx;
  right: 0;
  top: 143rpx;
}
.com-product-item .product-img {
  float: left;
  width: 221rpx;
  height: 221rpx;
  margin-right: 27rpx;
  overflow: hidden;
  border: 1rpx solid #f3f2f2;
  border-radius: 10rpx;
}
.com-product-item .product-img image {
  width: 221rpx;
  height: 221rpx;
}
.com-product-item .product-content .product-tit,
.com-product-item .product-content .product-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.com-product-item .product-content .product-tit {
  margin-top: 10rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: black;
}
.com-product-item .product-content .product-txt {
  margin-top: 14rpx;
  line-height: 32rpx;
  font-size: 25rpx;
  color: #999999;
}
.com-product-item .product-content .product-coupons {
  margin-top: 10rpx;
  position: relative;
  font-size: 24rpx;
  height: 40rpx;
  color: #FFF;
  overflow: hidden;
}
.com-product-item .product-content .product-coupons > view {
  width: 100%;
  height: 40rpx;
}
.com-product-item .product-content .product-coupons > view image {
  height: 40rpx;
  width: 137rpx;
  position: absolute;
  top: 0;
  left: 0rpx;
}
.com-product-item .product-content .product-coupons > view text {
  left: 50rpx;
  width: auto;
  height: 40rpx;
  position: absolute;
  top: 0;
  text-align: center;
  color: #fff;
  line-height: 41.5rpx;
  display: flex;
  white-space: nowrap;
  background: #ff7e78;
  padding-right: 5rpx;
  border-radius: 5rpx;
  font-weight: 100;
}
.com-product-item .product-content .product-oldprice {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.com-product-item .product-content .product-price {
  margin-top: -5rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.com-product-item .product-content .product-price .product-priceo {
  font-size: 24rpx;
  color: #4da68d;
  height: 40rpx;
}
.com-product-item .product-content .product-price .product-priceo text {
  font-size: 38rpx;
  font-weight: bold;
}
.com-product-item .product-content .product-price .product-pricet {
  font-size: 24rpx;
  color: white;
  height: 48rpx;
  padding: 0 20rpx;
  background: linear-gradient(to right, #6de2cd, #3cb6ad);
  border-radius: 48rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.com-product-item .product-content .product-price .product-pricet image {
  width: 29rpx;
  height: 29rpx;
  margin-right: 12rpx;
}
page {
  background: #000;
}
.shortvideo {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.shortvideo .back {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.3);
  border-radius: 50%;
  width: 60rpx;
  height: 60rpx;
  left: 50rpx;
  top: 68rpx;
  z-index: 5;
}
.shortvideo .back image {
  width: 20rpx;
}
.shortvideo .videobg {
  height: 100vh;
  width: 100vw;
  position: absolute;
  bottom: 0;
}
.shortvideo .video-info {
  position: absolute;
  top: 170rpx;
  left: 36rpx;
  right: 20rpx;
}
.shortvideo .video-info > view:nth-child(1) {
  height: 74rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 37rpx;
  padding-right: 20rpx;
  float: left;
}
.shortvideo .video-info > view:nth-child(1) image {
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
  float: left;
}
.shortvideo .video-info > view:nth-child(1) > view {
  float: left;
  margin-left: 16rpx;
  color: #FFF;
}
.shortvideo .video-info > view:nth-child(1) > view view:nth-child(1) {
  font-size: 28rpx;
  margin: 10rpx 0 6rpx 0;
}
.shortvideo .video-info > view:nth-child(1) > view view:nth-child(2) {
  font-size: 20rpx;
}
.shortvideo .video-info > view:nth-child(1) > view:last-child {
  font-size: 24rpx;
  line-height: 74rpx;
}
.shortvideo .replay {
  width: 228rpx;
  height: 42rpx;
  border-radius: 21rpx;
  background: rgba(0, 0, 0, 0.3);
  position: absolute;
  box-sizing: border-box;
  top: 270rpx;
  left: 36rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 27rpx 0 24rpx;
  color: #FFF;
}
.shortvideo .replay text:nth-child(1) {
  border-right: 1rpx solid #bbb9ba;
  line-height: 28rpx;
  padding: 0 12rpx 0 0;
  margin-right: 12rpx;
}
.shortvideo .replay text {
  font-size: 20rpx;
}
.shortvideo .bottom-btn {
  position: absolute;
  right: 47rpx;
  bottom: 80rpx;
  width: 80rpx;
}
.shortvideo .bottom-btn .like,
.shortvideo .bottom-btn .forward {
  width: 80rpx;
  height: 80rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 10rpx;
}
.shortvideo .bottom-btn .like image,
.shortvideo .bottom-btn .forward image {
  width: 54rpx;
  height: 50rpx;
}
.shortvideo .bottom-btn view {
  text-align: center;
  color: #FFF;
  text-shadow: 1rpx 1rpx 2rpx #000000;
  margin: 10rpx 0 30rpx;
  font-size: 22rpx;
}
.shortvideo .pop-class {
  border-radius: 14rpx;
  top: none;
  bottom: 40rpx;
}
.shortvideo .pop-content {
  position: absolute;
  background: #FFF;
  box-sizing: border-box;
  width: 578rpx;
  border-radius: 16rpx;
  bottom: 110rpx;
  padding: 20rpx;
}
.shortvideo .pop-content image {
  width: 148rpx;
  height: 148rpx;
  border-radius: 6rpx;
  float: left;
  margin-right: 15rpx;
}
.shortvideo .pop-content .price-subsify {
  font-size: 16rpx;
  color: #9c6e4d;
  background-color: #fff1d6;
  height: 28rpx;
  border-radius: 30rpx;
  line-height: 28rpx;
  display: inline-block;
  padding: 0 15rpx 0 15rpx;
}
.shortvideo .pop-content .price-subsify view {
  padding: 0 15rpx 0 15rpx;
}
.shortvideo .pop-content .tit {
  color: #000;
  font-size: 26rpx;
  height: 28rpx;
  line-height: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.shortvideo .pop-content .save {
  color: #4da68d;
  font-size: 20rpx;
  margin: 20rpx 0 20rpx 0;
}
.shortvideo .pop-content .save text:nth-child(1) {
  font-size: 24rpx;
}
.shortvideo .pop-content .save text:nth-child(2) {
  font-size: 20rpx;
}
.shortvideo .pop-content .detail {
  background: linear-gradient(to right, #38debf, #12a5bf);
  width: 98rpx;
  color: #FFF;
  font-size: 20rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  border-radius: 18rpx;
  position: absolute;
  bottom: 20rpx;
  right: 34rpx;
}
.shortvideo .pop-content .bottom {
  font-size: 24rpx;
  color: #4da68d;
}
.opctions {
  opacity: 0;
}
.share-pop .share-body {
  top: 12vh;
  top: calc(12vh + env(safe-area-inset-bottom));
}
