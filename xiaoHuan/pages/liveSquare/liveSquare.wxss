view,
text {
  font-family: -apple-system, BlinkMacSystemFont, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Segoe UI", "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.blue-green-btn {
  background: linear-gradient(to right, #38debf, #12a5bf);
}
.blue-purple-btn {
  background: linear-gradient(to right, #4d8be1, #889cf4);
}
view,
text {
  line-height: 1;
}
button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: 1;
}
button {
  border-radius: 0;
}
.banxin {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
}
.col:after {
  content: "";
  height: 0;
  clear: both;
  overflow: hidden;
  display: block;
  visibility: hidden;
}
.share-pop {
  position: fixed;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
}
.share-pop .share-body {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  top: calc(5vh);
  top: calc(5vh + env(safe-area-inset-bottom));
}
.share-pop .share-body .close {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.share-pop .share-body .close image {
  width: 50rpx;
  height: 50rpx;
}
.share-pop .share-body image {
  width: 80vw;
  height: 77vh;
  border-radius: 20rpx;
}
.share-pop .share-body button {
  position: relative;
  width: 80vw;
  height: 5vh !important;
  font-size: 25rpx;
  color: #FFF;
  line-height: 5vh;
  border-radius: 5rpx;
  background: linear-gradient(to right, #39dfbf, #13a6bf);
  text-align: center;
  margin-top: 3vh;
  /*margin-top: 40rpx;*/
}
.share-pop .share-body .share-btn {
  margin-top: 2vh;
}
.com-product-item:first-child {
  border-top: 0!important;
}
.com-product-item {
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-top: 1rpx dotted #dcdcdc;
}
.com-product-item .subsidy {
  top: 0;
  height: 34rpx;
  white-space: nowrap;
  border-radius: 17rpx;
  padding: 0 10rpx;
  color: #957052;
  font-size: 18rpx;
  background: #fcf1d8;
  position: absolute;
  line-height: 34rpx;
  right: 0;
  top: 143rpx;
}
.com-product-item .product-img {
  float: left;
  width: 221rpx;
  height: 221rpx;
  margin-right: 27rpx;
  overflow: hidden;
  border: 1rpx solid #f3f2f2;
  border-radius: 10rpx;
}
.com-product-item .product-img image {
  width: 221rpx;
  height: 221rpx;
}
.com-product-item .product-content .product-tit,
.com-product-item .product-content .product-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.com-product-item .product-content .product-tit {
  margin-top: 10rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: black;
}
.com-product-item .product-content .product-txt {
  margin-top: 14rpx;
  line-height: 32rpx;
  font-size: 25rpx;
  color: #999999;
}
.com-product-item .product-content .product-coupons {
  margin-top: 10rpx;
  position: relative;
  font-size: 24rpx;
  height: 40rpx;
  color: #FFF;
  overflow: hidden;
}
.com-product-item .product-content .product-coupons > view {
  width: 100%;
  height: 40rpx;
}
.com-product-item .product-content .product-coupons > view image {
  height: 40rpx;
  width: 137rpx;
  position: absolute;
  top: 0;
  left: 0rpx;
}
.com-product-item .product-content .product-coupons > view text {
  left: 50rpx;
  width: auto;
  height: 40rpx;
  position: absolute;
  top: 0;
  text-align: center;
  color: #fff;
  line-height: 41.5rpx;
  display: flex;
  white-space: nowrap;
  background: #ff7e78;
  padding-right: 5rpx;
  border-radius: 5rpx;
  font-weight: 100;
}
.com-product-item .product-content .product-oldprice {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.com-product-item .product-content .product-price {
  margin-top: -5rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.com-product-item .product-content .product-price .product-priceo {
  font-size: 24rpx;
  color: #4da68d;
  height: 40rpx;
}
.com-product-item .product-content .product-price .product-priceo text {
  font-size: 38rpx;
  font-weight: bold;
}
.com-product-item .product-content .product-price .product-pricet {
  font-size: 24rpx;
  color: white;
  height: 48rpx;
  padding: 0 20rpx;
  background: linear-gradient(to right, #6de2cd, #3cb6ad);
  border-radius: 48rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.com-product-item .product-content .product-price .product-pricet image {
  width: 29rpx;
  height: 29rpx;
  margin-right: 12rpx;
}
.van-tab--active {
  color: #4da68d !important;
}
page {
  background: #f3f3f3;
}
.nav {
  height: 98rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav .tab {
  height: 72rpx;
  width: 465rpx;
  border-radius: 36rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #666666;
  border: 1rpx solid #e1e1e1;
}
.nav .tab view {
  width: 235rpx;
  text-align: center;
  font-size: 36rpx;
  line-height: 36rpx;
  top: 20rpx;
}
.nav .tab view:first-child {
  z-index: 1;
  left: 0;
  box-sizing: border-box;
  border-right: 1rpx solid #e1e1e1;
}
.nav .tab view:nth-child(2) {
  z-index: 1;
  right: 0;
}
.nav .tab view:last-child {
  top: 58rpx;
  height: 5.2rpx;
  border-radius: 2.6rpx;
  transition: all 0.2s;
  width: 74rpx;
  position: absolute;
  background: #4da68d;
}
.nav .tab .left {
  left: 79rpx;
}
.nav .tab .right {
  left: 308rpx;
}
.nav .tab .check {
  color: #4da68d;
}
.product-list {
  position: fixed;
  top: 0rpx;
  left: 0;
  bottom: calc(100rpx + env(safe-area-inset-bottom));
  right: 0;
  overflow: auto;
  width: 750rpx;
}
.live-comment {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
  width: 712rpx;
  margin-top: 20rpx;
  overflow: hidden;
  position: relative;
}
.live-comment .comment-tit {
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
  color: #000;
  margin: 50rpx auto 40rpx;
  width: 212rpx;
  background: #FFF;
  position: relative;
  z-index: 1;
}
.live-comment .line {
  width: 488rpx;
  height: 2rpx;
  background: #cfcfcf;
  position: absolute;
  top: 68rpx;
  left: calc(50% - 244rpx);
}
.live-comment .comment-list {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
  width: 712rpx;
  border-radius: 0;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.live-comment .comment-list .live-item {
  width: 348rpx;
  height: 530rpx;
  border-radius: 21rpx;
  position: relative;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.live-comment .comment-list .live-item .main {
  width: 350rpx;
  height: 530rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
}
.live-comment .comment-list .live-item .main .imgs {
  width: 350rpx;
  height: 350rpx;
  display: flex;
  flex-wrap: wrap;
}
.live-comment .comment-list .live-item .main .imgs image {
  width: 173rpx;
  height: 173rpx;
}
.live-comment .comment-list .live-item .main image {
  width: 350rpx;
  height: 530rpx;
}
.live-comment .comment-list .live-item .main .m_coverimg {
  position: absolute;
  top: 310rpx;
  left: 0;
  width: 350rpx;
  height: 220rpx;
  background: white;
  padding: 0 20rpx;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_tit {
  margin-top: 70rpx;
  font-size: 28rpx;
  width: 320rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: black;
  margin-bottom: 15rpx;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_detail {
  color: #999999;
  font-size: 24rpx;
  width: 320rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 27rpx;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_watch {
  color: #666666;
  font-size: 20rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_watch .mw_img {
  width: 95rpx;
  height: 40rpx;
  margin-right: 10rpx;
  position: relative;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_watch .mw_img image {
  width: 40rpx;
  border-radius: 50%;
  position: absolute;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_watch .mw_img image:nth-child(1) {
  left: 0;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_watch .mw_img image:nth-child(2) {
  left: 27rpx;
}
.live-comment .comment-list .live-item .main .m_coverimg .mc_watch .mw_img image:nth-child(3) {
  left: 54rpx;
}
.live-comment .comment-list .live-item .live-foot {
  position: absolute;
  z-index: 0;
  bottom: 10rpx;
  color: #FFF;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}
.live-comment .comment-list .live-item .live-foot image {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  margin: 0 10rpx 0 10rpx;
}
.live-comment .comment-list .live-item .live-foot .live-name {
  width: 176rpx;
}
.live-comment .comment-list .live-item .live-foot .like-icon {
  width: 27rpx;
  height: 27rpx;
  margin: 0;
}
.live-comment .comment-list .live-item .live-foot .live-like {
  width: 80rpx;
  margin-left: 8rpx;
}
.live-comment .comment-list .live-item .tit-tag {
  width: 260rpx;
  height: 38rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 19rpx;
  position: absolute;
  font-size: 20rpx;
  top: 0rpx;
  left: 0rpx;
  line-height: 38rpx;
  color: #FFF;
}
.live-comment .comment-list .live-item .tit-tag .tag-left {
  width: 104rpx;
  height: 38rpx;
  float: left;
  background: skyblue;
  border-top-left-radius: 19rpx;
  border-bottom-left-radius: 19rpx;
  text-align: center;
  line-height: 38rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  justify-content: space-between;
  padding: 0 14rpx;
}
.live-comment .comment-list .live-item .tit-tag .tag-left image {
  width: 12rpx;
  height: 15rpx;
}
.live-comment .comment-list .live-item .tit-tag .tall {
  width: auto;
  background: linear-gradient(to right, #4d8be1, #889cf4);
}
.live-comment .comment-list .live-item .tit-tag .live-tag {
  background: linear-gradient(to right, #38debf, #12a5bf);
}
.live-comment .comment-list .live-item .tit-tag .tag-right {
  float: left;
  width: 150rpx;
  line-height: 38rpx;
  text-align: center;
}
.live-comment .comment-list .live-item .tit_two {
  top: 328rpx;
  left: 20rpx;
}
.live-comment .comment-list .live-item .mask {
  background: linear-gradient(to top, rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0));
  top: 0px;
  right: 0;
  left: 0;
  bottom: 0;
  position: absolute;
}
.live-comment .comment-list .live-item .content-foot {
  position: absolute;
  bottom: 16rpx;
  padding-left: 15rpx;
  left: 0;
  right: 0;
  color: #FFF;
}
.live-comment .comment-list .live-item .content-foot .price {
  font-size: 30rpx;
  margin-bottom: 6rpx;
}
.live-comment .comment-list .live-item .content-foot .price text:nth-child(1) {
  font-size: 58rpx;
}
.live-comment .comment-list .live-item .content-foot .shopname {
  font-size: 24rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.start-live {
  width: 152rpx;
  height: 66rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  right: 0;
  border-top-left-radius: 33rpx;
  bottom: 200rpx;
  border-bottom-left-radius: 33rpx;
  background: linear-gradient(to top, #4ba2bc, #70dbc0);
}
.start-live image {
  width: 50rpx;
  height: 38rpx;
}
.start-live view {
  margin-left: 6rpx;
  color: #FFF;
  font-size: 22rpx;
  width: 48rpx;
  line-height: 23rpx;
}
