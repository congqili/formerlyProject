view,
text {
  font-family: -apple-system, BlinkMacSystemFont, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Segoe UI", "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.blue-green-btn {
  background: linear-gradient(to right, #38debf, #12a5bf);
}
.blue-purple-btn {
  background: linear-gradient(to right, #4d8be1, #889cf4);
}
view,
text {
  line-height: 1;
}
button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: 1;
}
button {
  border-radius: 0;
}
.banxin {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
}
.col:after {
  content: "";
  height: 0;
  clear: both;
  overflow: hidden;
  display: block;
  visibility: hidden;
}
.share-pop {
  position: fixed;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
}
.share-pop .share-body {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  top: calc(5vh);
  top: calc(5vh + env(safe-area-inset-bottom));
}
.share-pop .share-body .close {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.share-pop .share-body .close image {
  width: 50rpx;
  height: 50rpx;
}
.share-pop .share-body image {
  width: 80vw;
  height: 77vh;
  border-radius: 20rpx;
}
.share-pop .share-body button {
  position: relative;
  width: 80vw;
  height: 5vh !important;
  font-size: 25rpx;
  color: #FFF;
  line-height: 5vh;
  border-radius: 5rpx;
  background: linear-gradient(to right, #39dfbf, #13a6bf);
  text-align: center;
  margin-top: 3vh;
  /*margin-top: 40rpx;*/
}
.share-pop .share-body .share-btn {
  margin-top: 2vh;
}
.com-product-item:first-child {
  border-top: 0!important;
}
.com-product-item {
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-top: 1rpx dotted #dcdcdc;
}
.com-product-item .subsidy {
  top: 0;
  height: 34rpx;
  white-space: nowrap;
  border-radius: 17rpx;
  padding: 0 10rpx;
  color: #957052;
  font-size: 18rpx;
  background: #fcf1d8;
  position: absolute;
  line-height: 34rpx;
  right: 0;
  top: 143rpx;
}
.com-product-item .product-img {
  float: left;
  width: 221rpx;
  height: 221rpx;
  margin-right: 27rpx;
  overflow: hidden;
  border: 1rpx solid #f3f2f2;
  border-radius: 10rpx;
}
.com-product-item .product-img image {
  width: 221rpx;
  height: 221rpx;
}
.com-product-item .product-content .product-tit,
.com-product-item .product-content .product-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.com-product-item .product-content .product-tit {
  margin-top: 10rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: black;
}
.com-product-item .product-content .product-txt {
  margin-top: 14rpx;
  line-height: 32rpx;
  font-size: 25rpx;
  color: #999999;
}
.com-product-item .product-content .product-coupons {
  margin-top: 10rpx;
  position: relative;
  font-size: 24rpx;
  height: 40rpx;
  color: #FFF;
  overflow: hidden;
}
.com-product-item .product-content .product-coupons > view {
  width: 100%;
  height: 40rpx;
}
.com-product-item .product-content .product-coupons > view image {
  height: 40rpx;
  width: 137rpx;
  position: absolute;
  top: 0;
  left: 0rpx;
}
.com-product-item .product-content .product-coupons > view text {
  left: 50rpx;
  width: auto;
  height: 40rpx;
  position: absolute;
  top: 0;
  text-align: center;
  color: #fff;
  line-height: 41.5rpx;
  display: flex;
  white-space: nowrap;
  background: #ff7e78;
  padding-right: 5rpx;
  border-radius: 5rpx;
  font-weight: 100;
}
.com-product-item .product-content .product-oldprice {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.com-product-item .product-content .product-price {
  margin-top: -5rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.com-product-item .product-content .product-price .product-priceo {
  font-size: 24rpx;
  color: #4da68d;
  height: 40rpx;
}
.com-product-item .product-content .product-price .product-priceo text {
  font-size: 38rpx;
  font-weight: bold;
}
.com-product-item .product-content .product-price .product-pricet {
  font-size: 24rpx;
  color: white;
  height: 48rpx;
  padding: 0 20rpx;
  background: linear-gradient(to right, #6de2cd, #3cb6ad);
  border-radius: 48rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.com-product-item .product-content .product-price .product-pricet image {
  width: 29rpx;
  height: 29rpx;
  margin-right: 12rpx;
}
page {
  background: #000;
}
.live-push {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
}
.start-live {
  overflow: hidden;
}
.start-live .mask-bg {
  position: fixed;
  z-index: -1;
  display: flex;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  justify-content: center;
  align-items: center;
  height: 100vh;
}
.start-live .mask-bg .videobg {
  width: 750rpx;
}
.start-live .mask-image {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100vh;
  width: 100vw;
  filter: blur(25rpx);
  z-index: -2;
}
.start-live .bottom-btn {
  width: 100%;
  height: 81rpx;
  position: fixed;
  bottom: 70rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0 36rpx;
  justify-content: space-between;
}
.start-live .bottom-btn > view {
  display: flex;
  width: 308rpx;
  justify-content: space-between;
}
.start-live .bottom-btn > view:first-child {
  width: 160rpx;
}
.start-live .bottom-btn .btn {
  width: 135rpx;
  height: 81rpx;
  border-radius: 6rpx;
  color: #FFF;
  text-align: center;
  line-height: 81rpx;
  font-size: 26rpx;
}
.start-live .bottom-btn .start {
  background: linear-gradient(to right, #3ae0c0, #10a3bf);
}
.start-live .bottom-btn .back {
  background: #898989;
}
.start-live .bottom-btn .fun-btn {
  font-size: 24rpx;
  color: #FFF;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 81rpx;
}
.start-live .bottom-btn .fun-btn image {
  height: 47rpx;
  width: 50rpx;
}
.start-live .live-info {
  margin-top: 150rpx;
  display: flex;
  position: relative;
  justify-content: space-between;
  padding: 0 36rpx;
  color: #FFF;
  font-size: 22rpx;
}
.start-live .live-info > view:nth-child(1) {
  width: 260rpx;
  height: 44rpx;
  background: rgba(0, 0, 0, 0.1);
  border-radius: 6rpx;
  line-height: 44rpx;
  text-indent: 10rpx;
}
.start-live .live-info > view:nth-child(1) view {
  float: left;
  width: 87rpx;
  height: 44rpx;
  background: linear-gradient(to right, #3ae0c0, #10a3bf);
  border-radius: 6rpx;
  text-align: center;
  line-height: 44rpx;
}
.start-live .live-info > view:nth-child(2) {
  width: 260rpx;
  height: 44rpx;
  background: rgba(0, 0, 0, 0.1);
  border-radius: 6rpx;
  text-align: center;
  line-height: 44rpx;
}
.start-live .live-fun {
  margin-top: 26rpx;
  display: flex;
  position: relative;
  padding: 0 40rpx;
  justify-content: space-between;
  align-items: center;
  color: #FFF;
}
.start-live .live-fun view:first-child {
  line-height: 52rpx;
}
.start-live .live-fun view:first-child image {
  height: 52rpx;
  float: left;
  margin-right: 18rpx;
}
.start-live .live-fun view:last-child {
  width: 360rpx;
  display: flex;
  justify-content: space-between;
}
.start-live .live-fun view:last-child image {
  height: 50rpx;
}
.pop-class {
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  overflow: hidden;
}
.pop-content {
  height: 277rpx;
  width: 750rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.pop-content .wrapper {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 39rpx;
  color: #fff;
  margin-bottom: 50rpx;
}
.pop-content .wrapper .top {
  width: 490rpx;
  margin-left: 28rpx;
}
.pop-content .custom-button {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background: #10a98c;
}
.product-content {
  overflow: auto;
  position: relative;
  padding-top: 86rpx;
}
.product-content .top-btn {
  height: 86rpx;
  background: #10a98c;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  top: 0;
  width: 100%;
  position: absolute;
}
.product-content .top-btn image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}
.product-content .product-list {
  overflow-x: hidden;
  overflow-y: auto;
  height: 830rpx;
}
.product-content .product-list .product-item:last-child {
  margin-bottom: 100rpx;
}
.product-content .product-list .product-item {
  display: flex;
  justify-content: space-between;
  padding: 50rpx 48rpx 0;
}
.product-content .product-list .product-item .img {
  position: relative;
  overflow: hidden;
}
.product-content .product-list .product-item .img image {
  width: 162rpx;
  height: 162rpx;
  border-radius: 5rpx;
}
.product-content .product-list .product-item .img .hot {
  position: absolute;
  top: 6rpx;
  left: 6rpx;
  width: 28rpx;
  height: 28rpx;
  text-align: center;
  line-height: 28rpx;
  color: #fff;
  background: #e92962;
  font-size: 16rpx;
  border-radius: 50%;
}
.product-content .product-list .product-item .img .tag {
  position: absolute;
  top: 0;
  left: 0;
  font-size: 18rpx;
  color: #fff;
  background: rgba(0, 0, 0, 0.3);
  text-align: center;
  line-height: 23rpx;
  height: 23rpx;
  padding: 0 7rpx;
  border-bottom-right-radius: 4rpx;
}
.product-content .product-list .product-item .product-desc {
  width: 452rpx;
}
.product-content .product-list .product-item .product-desc .product-name {
  color: #000;
  font-size: 25rpx;
  line-height: 33rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.product-content .product-list .product-item .product-desc .product-price {
  display: flex;
  justify-content: space-between;
  margin: 20rpx 0 22rpx;
}
.product-content .product-list .product-item .product-desc .product-price .price {
  color: #4da68d;
  font-size: 22rpx;
}
.product-content .product-list .product-item .product-desc .product-price .price text {
  font-size: 30rpx;
}
.product-content .product-list .product-item .product-desc .product-price .btn {
  width: 136rpx;
  height: 32rpx;
  background: linear-gradient(to right, #38debf, #12a5bf);
  border-radius: 16rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 15.7rpx;
  color: #fff;
}
.product-content .product-list .product-item .product-desc .operate {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.product-content .product-list .product-item .product-desc .operate view:first-child {
  color: #4da68d;
  font-size: 24rpx;
}
.product-content .product-list .product-item .product-desc .operate .set-top {
  width: 38rpx;
  height: 38rpx;
  border-radius: 50%;
}
.product-content .product-list .product-item .product-desc .operate .record {
  padding: 0 20rpx;
  border-radius: 19rpx;
  line-height: 38rpx;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  background: linear-gradient(to right, #38debf, #12a5bf);
}
