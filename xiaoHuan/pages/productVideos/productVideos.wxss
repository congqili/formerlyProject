view,
text {
  font-family: -apple-system, BlinkMacSystemFont, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Segoe UI", "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.blue-green-btn {
  background: linear-gradient(to right, #38debf, #12a5bf);
}
.blue-purple-btn {
  background: linear-gradient(to right, #4d8be1, #889cf4);
}
view,
text {
  line-height: 1;
}
button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: 1;
}
button {
  border-radius: 0;
}
.banxin {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
}
.col:after {
  content: "";
  height: 0;
  clear: both;
  overflow: hidden;
  display: block;
  visibility: hidden;
}
.share-pop {
  position: fixed;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
}
.share-pop .share-body {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  top: calc(5vh);
  top: calc(5vh + env(safe-area-inset-bottom));
}
.share-pop .share-body .close {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.share-pop .share-body .close image {
  width: 50rpx;
  height: 50rpx;
}
.share-pop .share-body image {
  width: 80vw;
  height: 77vh;
  border-radius: 20rpx;
}
.share-pop .share-body button {
  position: relative;
  width: 80vw;
  height: 5vh !important;
  font-size: 25rpx;
  color: #FFF;
  line-height: 5vh;
  border-radius: 5rpx;
  background: linear-gradient(to right, #39dfbf, #13a6bf);
  text-align: center;
  margin-top: 3vh;
  /*margin-top: 40rpx;*/
}
.share-pop .share-body .share-btn {
  margin-top: 2vh;
}
.com-product-item:first-child {
  border-top: 0!important;
}
.com-product-item {
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-top: 1rpx dotted #dcdcdc;
}
.com-product-item .subsidy {
  top: 0;
  height: 34rpx;
  white-space: nowrap;
  border-radius: 17rpx;
  padding: 0 10rpx;
  color: #957052;
  font-size: 18rpx;
  background: #fcf1d8;
  position: absolute;
  line-height: 34rpx;
  right: 0;
  top: 143rpx;
}
.com-product-item .product-img {
  float: left;
  width: 221rpx;
  height: 221rpx;
  margin-right: 27rpx;
  overflow: hidden;
  border: 1rpx solid #f3f2f2;
  border-radius: 10rpx;
}
.com-product-item .product-img image {
  width: 221rpx;
  height: 221rpx;
}
.com-product-item .product-content .product-tit,
.com-product-item .product-content .product-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.com-product-item .product-content .product-tit {
  margin-top: 10rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: black;
}
.com-product-item .product-content .product-txt {
  margin-top: 14rpx;
  line-height: 32rpx;
  font-size: 25rpx;
  color: #999999;
}
.com-product-item .product-content .product-coupons {
  margin-top: 10rpx;
  position: relative;
  font-size: 24rpx;
  height: 40rpx;
  color: #FFF;
  overflow: hidden;
}
.com-product-item .product-content .product-coupons > view {
  width: 100%;
  height: 40rpx;
}
.com-product-item .product-content .product-coupons > view image {
  height: 40rpx;
  width: 137rpx;
  position: absolute;
  top: 0;
  left: 0rpx;
}
.com-product-item .product-content .product-coupons > view text {
  left: 50rpx;
  width: auto;
  height: 40rpx;
  position: absolute;
  top: 0;
  text-align: center;
  color: #fff;
  line-height: 41.5rpx;
  display: flex;
  white-space: nowrap;
  background: #ff7e78;
  padding-right: 5rpx;
  border-radius: 5rpx;
  font-weight: 100;
}
.com-product-item .product-content .product-oldprice {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.com-product-item .product-content .product-price {
  margin-top: -5rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.com-product-item .product-content .product-price .product-priceo {
  font-size: 24rpx;
  color: #4da68d;
  height: 40rpx;
}
.com-product-item .product-content .product-price .product-priceo text {
  font-size: 38rpx;
  font-weight: bold;
}
.com-product-item .product-content .product-price .product-pricet {
  font-size: 24rpx;
  color: white;
  height: 48rpx;
  padding: 0 20rpx;
  background: linear-gradient(to right, #6de2cd, #3cb6ad);
  border-radius: 48rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.com-product-item .product-content .product-price .product-pricet image {
  width: 29rpx;
  height: 29rpx;
  margin-right: 12rpx;
}
page {
  background: #f3f3f3;
}
.comment {
  background: #f3f3f3;
  overflow: hidden;
}
.comment .goods-zoom {
  position: relative;
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
  overflow: hidden;
  background: #FFF;
  margin-top: 20rpx;
}
.comment .goods-zoom image {
  width: 100%;
  height: 710rpx;
  display: block;
}
.comment .goods-zoom > view {
  box-sizing: border-box;
  padding: 0 20rpx;
}
.comment .goods-zoom .goods-tit {
  width: 700rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 34rpx;
  font-weight: bold;
  color: #000;
  margin: 34rpx 0 16rpx;
}
.comment .goods-zoom .goods-info {
  font-size: 25rpx;
  color: #2f2e2e;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.comment .goods-zoom .goods-price {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 30rpx 0 30rpx;
}
.comment .goods-zoom .goods-price view:nth-child(1) {
  font-size: 26rpx;
  color: #10a98c;
}
.comment .goods-zoom .goods-price view:nth-child(1) text {
  font-size: 30rpx;
}
.comment .goods-zoom .goods-price .now-price {
  color: #0fa98c;
  margin-left: 10rpx;
  display: flex;
  align-items: flex-end;
}
.comment .goods-zoom .goods-price .now-price text:nth-child(1),
.comment .goods-zoom .goods-price .now-price text:nth-child(3) {
  font-size: 30rpx;
}
.comment .goods-zoom .goods-price .now-price text:nth-child(2) {
  font-size: 40rpx;
  font-weight: bold;
  transform: translateY(1rpx);
}
.comment .goods-zoom .goods-price .now-price text:nth-child(3) {
  font-weight: bold;
}
.comment .goods-zoom .goods-price .buy-btn {
  background: linear-gradient(to right, #70dbc0, #4da4bc);
  width: 176rpx;
  height: 52rpx;
  font-size: 22rpx;
  text-align: center;
  line-height: 52rpx;
  border-radius: 26rpx;
  color: #FFF;
}
.comment .live-comment {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background: #FFF;
  overflow: hidden;
  position: relative;
}
.comment .live-comment .comment-tit {
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
  color: #000;
  margin: 50rpx auto 40rpx;
  width: 212rpx;
  background: #FFF;
  position: relative;
  z-index: 1;
}
.comment .live-comment .line {
  width: 488rpx;
  height: 2rpx;
  background: #cfcfcf;
  position: absolute;
  top: 68rpx;
  left: calc(50% - 244rpx);
}
.comment .live-comment .comment-list {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
  border-radius: 0;
  margin-top: 20rpx;
  padding-bottom: 12rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.comment .live-comment .comment-list .live-item {
  width: 350rpx;
  height: 530rpx;
  border-radius: 15rpx;
  background: #c6c3e4;
  position: relative;
  overflow: hidden;
  margin-bottom: 10rpx;
}
.comment .live-comment .comment-list .live-item .mask {
  background: linear-gradient(to top, rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0));
  top: 0px;
  right: 0;
  left: 0;
  bottom: 0;
  position: absolute;
}
.comment .live-comment .comment-list .live-item .main {
  width: 350rpx;
  height: 530rpx;
}
.comment .live-comment .comment-list .live-item .main image {
  width: 350rpx;
  height: 530rpx;
}
.comment .live-comment .comment-list .live-item .live-foot {
  position: absolute;
  z-index: 10;
  bottom: 7rpx;
  color: #FFF;
  font-size: 24rpx;
}
.comment .live-comment .comment-list .live-item .live-foot image {
  float: left;
  background: skyblue;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  margin: 0 10rpx 10rpx 10rpx;
}
.comment .live-comment .comment-list .live-item .live-foot view {
  float: left;
  line-height: 36rpx;
}
.comment .live-comment .comment-list .live-item .live-foot .live-name {
  width: 176rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.comment .live-comment .comment-list .live-item .tit-tag {
  z-index: 10;
  width: 260rpx;
  height: 38rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 19rpx;
  position: absolute;
  font-size: 20rpx;
  top: 0rpx;
  left: 0rpx;
  line-height: 38rpx;
  color: #FFF;
}
.comment .live-comment .comment-list .live-item .tit-tag .tag-left {
  width: 104rpx;
  height: 38rpx;
  float: left;
  background: linear-gradient(to right, #4d8be1, #889cf4);
  border-top-left-radius: 19rpx;
  border-bottom-left-radius: 19rpx;
  text-align: center;
  line-height: 38rpx;
}
.comment .live-comment .comment-list .live-item .tit-tag .tag-right {
  float: left;
  width: 150rpx;
  text-align: center;
  line-height: 38rpx;
}
.comment .live-comment .comment-list .live-item .content-foot {
  position: absolute;
  bottom: 16rpx;
  padding-left: 15rpx;
  color: #FFF;
}
.comment .live-comment .comment-list .live-item .content-foot .price {
  font-size: 30rpx;
}
.comment .live-comment .comment-list .live-item .content-foot .price text:nth-child(1) {
  font-size: 60rpx;
}
.comment .live-comment .comment-list .live-item .content-foot .shopname {
  font-size: 24rpx;
}
.opctions {
  opacity: 0;
}
.detail-foot {
  height: 168rpx;
  position: fixed;
  bottom: 0;
  background: #FFF;
  left: 0;
  z-index: 10;
  right: 0;
  padding: 0 20rpx;
  border-top: 1rpx solid #e6e6e6;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.detail-foot view {
  width: 346rpx;
  height: 80rpx;
  border-radius: 10rpx;
  color: #FFF;
  text-align: center;
  line-height: 80rpx;
  letter-spacing: 14rpx;
  font-size: 30rpx;
  background: linear-gradient(to right, #39dfbf, #13a6c0);
}
.price-subsify {
  position: absolute;
  bottom: 7rpx;
  font-size: 18rpx;
  color: #9c6e4d;
  background-color: #fff1d6;
  height: 30rpx;
  padding: 0 15rpx 0 15rpx;
  border-radius: 30rpx;
  line-height: 30rpx !important;
}
