view,
text {
  font-family: -apple-system, BlinkMacSystemFont, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Segoe UI", "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.blue-green-btn {
  background: linear-gradient(to right, #38debf, #12a5bf);
}
.blue-purple-btn {
  background: linear-gradient(to right, #4d8be1, #889cf4);
}
view,
text {
  line-height: 1;
}
button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: 1;
}
button {
  border-radius: 0;
}
.banxin {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
}
.col:after {
  content: "";
  height: 0;
  clear: both;
  overflow: hidden;
  display: block;
  visibility: hidden;
}
.share-pop {
  position: fixed;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
}
.share-pop .share-body {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  top: calc(5vh);
  top: calc(5vh + env(safe-area-inset-bottom));
}
.share-pop .share-body .close {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.share-pop .share-body .close image {
  width: 50rpx;
  height: 50rpx;
}
.share-pop .share-body image {
  width: 80vw;
  height: 77vh;
  border-radius: 20rpx;
}
.share-pop .share-body button {
  position: relative;
  width: 80vw;
  height: 5vh !important;
  font-size: 25rpx;
  color: #FFF;
  line-height: 5vh;
  border-radius: 5rpx;
  background: linear-gradient(to right, #39dfbf, #13a6bf);
  text-align: center;
  margin-top: 3vh;
  /*margin-top: 40rpx;*/
}
.share-pop .share-body .share-btn {
  margin-top: 2vh;
}
.com-product-item:first-child {
  border-top: 0!important;
}
.com-product-item {
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-top: 1rpx dotted #dcdcdc;
}
.com-product-item .subsidy {
  top: 0;
  height: 34rpx;
  white-space: nowrap;
  border-radius: 17rpx;
  padding: 0 10rpx;
  color: #957052;
  font-size: 18rpx;
  background: #fcf1d8;
  position: absolute;
  line-height: 34rpx;
  right: 0;
  top: 143rpx;
}
.com-product-item .product-img {
  float: left;
  width: 221rpx;
  height: 221rpx;
  margin-right: 27rpx;
  overflow: hidden;
  border: 1rpx solid #f3f2f2;
  border-radius: 10rpx;
}
.com-product-item .product-img image {
  width: 221rpx;
  height: 221rpx;
}
.com-product-item .product-content .product-tit,
.com-product-item .product-content .product-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.com-product-item .product-content .product-tit {
  margin-top: 10rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: black;
}
.com-product-item .product-content .product-txt {
  margin-top: 14rpx;
  line-height: 32rpx;
  font-size: 25rpx;
  color: #999999;
}
.com-product-item .product-content .product-coupons {
  margin-top: 10rpx;
  position: relative;
  font-size: 24rpx;
  height: 40rpx;
  color: #FFF;
  overflow: hidden;
}
.com-product-item .product-content .product-coupons > view {
  width: 100%;
  height: 40rpx;
}
.com-product-item .product-content .product-coupons > view image {
  height: 40rpx;
  width: 137rpx;
  position: absolute;
  top: 0;
  left: 0rpx;
}
.com-product-item .product-content .product-coupons > view text {
  left: 50rpx;
  width: auto;
  height: 40rpx;
  position: absolute;
  top: 0;
  text-align: center;
  color: #fff;
  line-height: 41.5rpx;
  display: flex;
  white-space: nowrap;
  background: #ff7e78;
  padding-right: 5rpx;
  border-radius: 5rpx;
  font-weight: 100;
}
.com-product-item .product-content .product-oldprice {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.com-product-item .product-content .product-price {
  margin-top: -5rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.com-product-item .product-content .product-price .product-priceo {
  font-size: 24rpx;
  color: #4da68d;
  height: 40rpx;
}
.com-product-item .product-content .product-price .product-priceo text {
  font-size: 38rpx;
  font-weight: bold;
}
.com-product-item .product-content .product-price .product-pricet {
  font-size: 24rpx;
  color: white;
  height: 48rpx;
  padding: 0 20rpx;
  background: linear-gradient(to right, #6de2cd, #3cb6ad);
  border-radius: 48rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.com-product-item .product-content .product-price .product-pricet image {
  width: 29rpx;
  height: 29rpx;
  margin-right: 12rpx;
}
.van-toast--icon {
  width: 270rpx!important;
}
.history-detail .content .info {
  width: 710rpx;
  border-radius: 12rpx;
  margin: 0 auto;
  overflow: hidden;
  background: linear-gradient(to right, #5fe4c9, #3eb3c9);
}
.history-detail .content .info view {
  font-size: 32rpx;
  color: #FFF;
  margin: 36rpx 0 30rpx;
  display: flex;
  justify-content: flex-start;
  padding: 0 0 0 44rpx;
}
.history-detail .content .info view text:first-child {
  width: 140rpx;
}
.history-detail .content .info view text:last-child {
  width: 300rpx;
  margin-left: 50rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.history-detail .back-look {
  box-sizing: border-box;
  width: 710rpx;
  background: #FFF;
  position: relative;
  border-radius: 12rpx;
  margin: 0 auto;
  overflow: hidden;
  padding-bottom: 10rpx;
}
.history-detail .back-look .back-tit {
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
  color: #000;
  margin: 50rpx auto 40rpx;
  width: 212rpx;
  background: #FFF;
  position: relative;
  z-index: 1;
}
.history-detail .back-look .line {
  width: 488rpx;
  height: 1rpx;
  background: rgba(0, 0, 0, 0.2);
  position: absolute;
  top: 68rpx;
  left: calc(50% - 244rpx);
}
.history-detail .back-look .live-list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.history-detail .back-look .live-list .live-item {
  width: 350rpx;
  height: 530rpx;
  border-radius: 15rpx;
  background: #c6c3e4;
  position: relative;
  overflow: hidden;
  margin-bottom: 10rpx;
}
.history-detail .back-look .live-list .live-item .main {
  width: 350rpx;
  height: 530rpx;
}
.history-detail .back-look .live-list .live-item .main video {
  width: 350rpx;
  height: 530rpx;
}
.history-detail .back-look .live-list .live-item .live-foot {
  position: absolute;
  z-index: 10;
  bottom: 7rpx;
  color: #FFF;
  font-size: 24rpx;
}
.history-detail .back-look .live-list .live-item .live-foot image {
  float: left;
  background: skyblue;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  margin: 0 10rpx 10rpx 10rpx;
}
.history-detail .back-look .live-list .live-item .live-foot view {
  float: left;
  line-height: 36rpx;
}
.history-detail .back-look .live-list .live-item .live-foot .live-name {
  width: 176rpx;
}
.history-detail .back-look .live-list .live-item .tit-tag {
  z-index: 10;
  width: 260rpx;
  height: 38rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 19rpx;
  position: absolute;
  font-size: 20rpx;
  top: 0rpx;
  left: 0rpx;
  line-height: 38rpx;
  color: #FFF;
}
.history-detail .back-look .live-list .live-item .tit-tag .tag-left {
  width: auto;
  height: 38rpx;
  float: left;
  background: linear-gradient(to right, #4d8be1, #889cf4);
  border-top-left-radius: 19rpx;
  border-bottom-left-radius: 19rpx;
  text-align: center;
  line-height: 38rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  justify-content: space-between;
  padding: 0 14rpx;
}
.history-detail .back-look .live-list .live-item .tit-tag .tag-left image {
  width: 12rpx;
  height: 15rpx;
}
.history-detail .back-look .live-list .live-item .tit-tag .tall {
  width: auto;
}
.history-detail .back-look .live-list .live-item .tit-tag .tag-right {
  float: left;
  width: 150rpx;
  line-height: 38rpx;
  text-align: center;
}
.history-detail .back-look .live-list .live-item .content-foot {
  position: absolute;
  bottom: 16rpx;
  padding-left: 15rpx;
  color: #FFF;
}
.history-detail .back-look .live-list .live-item .content-foot .price {
  font-size: 30rpx;
}
.history-detail .back-look .live-list .live-item .content-foot .price text:nth-child(1) {
  font-size: 60rpx;
}
.history-detail .back-look .live-list .live-item .content-foot .shopname {
  font-size: 24rpx;
}
