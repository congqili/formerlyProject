view,
text {
    font-family: font;
}
.blue-green-btn {
    background: linear-gradient(to right, #38debf, #12a5bf);
}
.blue-purple-btn {
    background: linear-gradient(to right, #4d8be1, #889cf4);
}
view,
text {
    line-height: 1;
}
button::after {
    border: none;
}
button {
    background-color: transparent;
    padding-left: 0;
    padding-right: 0;
    line-height: 1;
}
button {
    border-radius: 0;
}
.banxin {
    width: 710rpx;
    margin: 0 auto;
    border-radius: 15rpx;
}
.col:after {
    content: "";
    height: 0;
    clear: both;
    overflow: hidden;
    display: block;
    visibility: hidden;
}
.share-pop {
    position: fixed;
    background: rgba(0, 0, 0, 0.7);
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 1000;
}
.share-pop .share-body {
    position: absolute;
    left: 50%;
    transform: translate(-50%);
    top: calc(5vh);
    top: calc(5vh + env(safe-area-inset-bottom));
}
.share-pop .share-body image {
    width: 80vw;
    height: 77vh;
    border-radius: 20rpx;
}
.share-pop .share-body button {
    position: relative;
    width: 80vw;
    height: 5vh !important;
    font-size: 25rpx;
    color: #FFF;
    line-height: 5vh;
    border-radius: 5rpx;
    background: linear-gradient(to right, #39dfbf, #13a6bf);
    text-align: center;
    margin-top: 3vh;
    /*margin-top: 40rpx;*/
}
.share-pop .share-body .share-btn {
    margin-top: 2vh;
}
.share-pop .close {
    position: absolute;
    right: 20rpx;
    top: 10rpx;
    color: #FFF;
    font-size: 60rpx;
    line-height: 54rpx;
    background: rgba(0, 0, 0, 0.8);
    width: 60rpx;
    height: 60rpx;
    text-align: center;
    z-index: 1001;
    border-radius: 60rpx;
    display: none;
}
.com-product-item:first-child {
    border-top: 0!important;
}
.com-product-item {
    padding: 20rpx 0;
    overflow: hidden;
    position: relative;
    border-top: 1rpx dotted #dcdcdc;
}
.com-product-item .subsidy {
    top: 0;
    height: 34rpx;
    white-space: nowrap;
    border-radius: 17rpx;
    padding: 0 10rpx;
    color: #957052;
    font-size: 18rpx;
    background: #fcf1d8;
    position: absolute;
    line-height: 34rpx;
    right: 0;
    top: 143rpx;
}
.com-product-item .product-img {
    float: left;
    width: 221rpx;
    height: 221rpx;
    margin-right: 27rpx;
    overflow: hidden;
    border: 1rpx solid #f3f2f2;
    border-radius: 10rpx;
}
.com-product-item .product-img image {
    width: 221rpx;
    height: 221rpx;
}
.com-product-item .product-content .product-tit,
.com-product-item .product-content .product-txt {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.com-product-item .product-content .product-tit {
    margin-top: 10rpx;
    line-height: 32rpx;
    font-size: 28rpx;
    color: black;
}
.com-product-item .product-content .product-txt {
    margin-top: 14rpx;
    line-height: 32rpx;
    font-size: 25rpx;
    color: #999999;
}
.com-product-item .product-content .product-coupons {
    margin-top: 10rpx;
    position: relative;
    font-size: 24rpx;
    height: 40rpx;
    color: #FFF;
    overflow: hidden;
}
.com-product-item .product-content .product-coupons > view {
    width: 100%;
    height: 40rpx;
}
.com-product-item .product-content .product-coupons > view image {
    height: 40rpx;
    width: 137rpx;
    position: absolute;
    top: 0;
    left: 0rpx;
}
.com-product-item .product-content .product-coupons > view text {
    left: 50rpx;
    width: auto;
    height: 40rpx;
    position: absolute;
    top: 0;
    text-align: center;
    color: #fff;
    line-height: 41.5rpx;
    display: flex;
    white-space: nowrap;
    background: #ff7e78;
    padding-right: 5rpx;
    border-radius: 5rpx;
    font-weight: 100;
}
.com-product-item .product-content .product-oldprice {
    margin-top: 16rpx;
    font-size: 24rpx;
    color: #999999;
    text-decoration: line-through;
}
.com-product-item .product-content .product-price {
    margin-top: -5rpx;
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}
.com-product-item .product-content .product-price .product-priceo {
    font-size: 24rpx;
    color: #4da68d;
    height: 40rpx;
}
.com-product-item .product-content .product-price .product-priceo text {
    font-size: 38rpx;
    font-weight: bold;
}
.com-product-item .product-content .product-price .product-pricet {
    font-size: 24rpx;
    color: white;
    height: 48rpx;
    padding: 0 20rpx;
    background: linear-gradient(to right, #6de2cd, #3cb6ad);
    border-radius: 48rpx;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
}
.com-product-item .product-content .product-price .product-pricet image {
    width: 29rpx;
    height: 29rpx;
    margin-right: 12rpx;
}
page {
    background: #f9f7f3;
}
.keyChain {
    color: #b0b0b0;
    font-size: 18rpx;
    margin-top: 30rpx;
}
.keyChain .title-item {
    width: 706rpx;
    background-color: #FFF;
    border: 1rpx solid rgba(100, 100, 100, 0.2);
    margin: 0 auto 20rpx;
}
.keyChain .title-item input {
    padding: 20rpx;
    color: #000;
    font-size: 30rpx;
}
.keyChain .kc-area {
    position: relative;
}
.keyChain .kc-area .text-area {
    width: 706rpx;
    height: 474rpx;
    margin: 0 auto;
    border: 1rpx solid rgba(100, 100, 100, 0.2);
    border-radius: 5rpx;
    background: white;
    font-size: 30rpx;
    box-sizing: border-box;
    padding: 45rpx 20rpx 20rpx 20rpx;
    color: black;
}
.keyChain .kc-area .writeNum {
    position: absolute;
    top: 20rpx;
    left: 45rpx;
    font-size: 28rpx;
    color: #e5e5e5;
}
.keyChain .kc-btn {
    font-size: 24rpx;
}
.keyChain .kc-btn view {
    width: 375rpx;
    height: 58rpx;
    line-height: 58rpx;
    text-align: center;
    background: linear-gradient(to right, #38debf, #12a5bf);
    border-radius: 10rpx;
    margin: 30rpx auto 50rpx;
    color: white;
}
.keyChain .kc-tit {
    font-size: 28rpx;
}
.keyChain .kc-tit text {
    width: 700rpx;
    display: block;
    margin: 0 auto;
    font-size: 24rpx;
    line-height: 32rpx;
}
