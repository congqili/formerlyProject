view,
text {
  font-family: -apple-system, BlinkMacSystemFont, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Segoe UI", "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.blue-green-btn {
  background: linear-gradient(to right, #38debf, #12a5bf);
}
.blue-purple-btn {
  background: linear-gradient(to right, #4d8be1, #889cf4);
}
view,
text {
  line-height: 1;
}
button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: 1;
}
button {
  border-radius: 0;
}
.banxin {
  width: 710rpx;
  margin: 0 auto;
  border-radius: 15rpx;
}
.col:after {
  content: "";
  height: 0;
  clear: both;
  overflow: hidden;
  display: block;
  visibility: hidden;
}
.share-pop {
  position: fixed;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
}
.share-pop .share-body {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  top: calc(5vh);
  top: calc(5vh + env(safe-area-inset-bottom));
}
.share-pop .share-body .close {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.share-pop .share-body .close image {
  width: 50rpx;
  height: 50rpx;
}
.share-pop .share-body image {
  width: 80vw;
  height: 77vh;
  border-radius: 20rpx;
}
.share-pop .share-body button {
  position: relative;
  width: 80vw;
  height: 5vh !important;
  font-size: 25rpx;
  color: #FFF;
  line-height: 5vh;
  border-radius: 5rpx;
  background: linear-gradient(to right, #39dfbf, #13a6bf);
  text-align: center;
  margin-top: 3vh;
  /*margin-top: 40rpx;*/
}
.share-pop .share-body .share-btn {
  margin-top: 2vh;
}
.com-product-item:first-child {
  border-top: 0!important;
}
.com-product-item {
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-top: 1rpx dotted #dcdcdc;
}
.com-product-item .subsidy {
  top: 0;
  height: 34rpx;
  white-space: nowrap;
  border-radius: 17rpx;
  padding: 0 10rpx;
  color: #957052;
  font-size: 18rpx;
  background: #fcf1d8;
  position: absolute;
  line-height: 34rpx;
  right: 0;
  top: 143rpx;
}
.com-product-item .product-img {
  float: left;
  width: 221rpx;
  height: 221rpx;
  margin-right: 27rpx;
  overflow: hidden;
  border: 1rpx solid #f3f2f2;
  border-radius: 10rpx;
}
.com-product-item .product-img image {
  width: 221rpx;
  height: 221rpx;
}
.com-product-item .product-content .product-tit,
.com-product-item .product-content .product-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.com-product-item .product-content .product-tit {
  margin-top: 10rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: black;
}
.com-product-item .product-content .product-txt {
  margin-top: 14rpx;
  line-height: 32rpx;
  font-size: 25rpx;
  color: #999999;
}
.com-product-item .product-content .product-coupons {
  margin-top: 10rpx;
  position: relative;
  font-size: 24rpx;
  height: 40rpx;
  color: #FFF;
  overflow: hidden;
}
.com-product-item .product-content .product-coupons > view {
  width: 100%;
  height: 40rpx;
}
.com-product-item .product-content .product-coupons > view image {
  height: 40rpx;
  width: 137rpx;
  position: absolute;
  top: 0;
  left: 0rpx;
}
.com-product-item .product-content .product-coupons > view text {
  left: 50rpx;
  width: auto;
  height: 40rpx;
  position: absolute;
  top: 0;
  text-align: center;
  color: #fff;
  line-height: 41.5rpx;
  display: flex;
  white-space: nowrap;
  background: #ff7e78;
  padding-right: 5rpx;
  border-radius: 5rpx;
  font-weight: 100;
}
.com-product-item .product-content .product-oldprice {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.com-product-item .product-content .product-price {
  margin-top: -5rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.com-product-item .product-content .product-price .product-priceo {
  font-size: 24rpx;
  color: #4da68d;
  height: 40rpx;
}
.com-product-item .product-content .product-price .product-priceo text {
  font-size: 38rpx;
  font-weight: bold;
}
.com-product-item .product-content .product-price .product-pricet {
  font-size: 24rpx;
  color: white;
  height: 48rpx;
  padding: 0 20rpx;
  background: linear-gradient(to right, #6de2cd, #3cb6ad);
  border-radius: 48rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.com-product-item .product-content .product-price .product-pricet image {
  width: 29rpx;
  height: 29rpx;
  margin-right: 12rpx;
}
.member-level .level-top {
  margin: 0 30rpx;
  background: linear-gradient(to left, #acb6bf, #8292a1);
  border-radius: 16rpx;
  overflow: hidden;
}
.member-level .level-top .advator {
  display: flex;
  margin: 25rpx 0 46rpx 39rpx;
  align-items: center;
  color: #FFF;
}
.member-level .level-top .advator image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  border: 2rpx solid rgba(255, 255, 255, 0.4);
}
.member-level .level-top .linear-bar {
  position: relative;
}
.member-level .level-top .linear-bar .bar-dotted {
  height: 3rpx;
  background: #9199a0;
  display: flex;
  justify-content: space-between;
  padding: 0 66rpx;
  align-items: center;
}
.member-level .level-top .linear-bar .bar-dotted view {
  height: 5rpx;
  width: 5rpx;
  border-radius: 50%;
  background: #9199a0;
}
.member-level .level-top .linear-bar .bar-text {
  padding: 0 22rpx;
  color: #FFF;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  margin: 10rpx 0 22rpx;
}
.member-level .level-top .linear-bar .bar {
  width: 300rpx;
  background: #333;
  height: 3rpx;
  position: absolute;
}
.member-level .level-detail > view:first-child {
  margin: 38rpx 0 54rpx;
  text-align: center;
  font-size: 30rpx;
  color: #8f8f8f;
}
.member-level .level-detail > view:first-child text {
  margin: 0 10rpx;
  color: #393939;
}
.member-level .level-detail .need-num {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 100rpx;
}
.member-level .level-detail .need-num .num-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.member-level .level-detail .need-num .num-item view:first-child {
  height: 130rpx;
  width: 130rpx;
  font-size: 37rpx;
  line-height: 130rpx;
  text-align: center;
  border-radius: 50%;
  border: 8rpx solid #9bc6ba;
}
.member-level .level-detail .need-num .num-item view:last-child {
  text-align: center;
  font-size: 30rpx;
  margin-top: 26rpx;
}
.member-level .member-power > view:first-child {
  text-align: center;
  font-style: 34rpx;
  margin: 90rpx 0 44rpx;
  color: #000;
}
.member-level .member-power .power-list {
  display: flex;
  padding: 12rpx 120rpx 12rpx 30rpx;
  justify-content: space-between;
  box-shadow: 1rpx 1rpx 2rpx #000;
}
.member-level .member-power .power-list .power-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.member-level .member-power .power-list .power-item image {
  width: 56rpx;
  height: 56rpx;
}
.member-level .member-power .power-list .power-item view:first-child {
  text-align: center;
  color: #8d8d8d;
  font-size: 24rpx;
  margin: 38rpx 0 34rpx;
}
.member-level .member-power .power-list .power-item view:last-child {
  text-align: center;
  font-size: 34rpx;
  color: #000;
  margin-bottom: 44rpx;
}
.member-level .member-power .power-list .power-item:first-child {
  width: 165rpx;
  border-right: 1rpx #eeeeee solid;
}
