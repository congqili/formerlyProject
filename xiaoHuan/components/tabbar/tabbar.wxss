.wrapper {
  position: fixed;
  bottom: 0;
  z-index: 100;
  left: 0;
  right: 0;
  height: 100rpx;
  overflow: hidden;
  background: #FFF;
  border-top: 1rpx solid #dcdcdc;
  padding-bottom: calc(constant(safe-area-inset-bottom) + 0rpx);
  padding-bottom: calc(env(safe-area-inset-bottom) + 0rpx);
}
.wrapper view,
.wrapper text {
  line-height: 1;
}
.wrapper .bgimg {
  position: absolute;
  z-index: -1;
  top: 0;
  width: 100%;
  height: 188rpx;
}
.wrapper .tabbar {
  width: 100%;
  display: flex;
  padding: 0 20rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.wrapper .tabbar .btn {
  width: 160rpx;
  display: flex;
  padding-top: 15rpx;
  flex-direction: column;
  align-items: center;
  font-size: 20rpx;
  color: #999999;
  justify-content: space-between;
}
.wrapper .tabbar .btn navigator {
  width: 160rpx;
}
.wrapper .tabbar .btn navigator view {
  text-align: center;
}
.wrapper .tabbar .btn .tab-wrapper {
  width: 100%;
  height: 40rpx;
  overflow: hidden;
  margin-bottom: 8rpx;
}
.wrapper .tabbar .btn .tab-wrapper image {
  height: 40rpx;
  width: 100%;
}
.wrapper .tabbar .check {
  color: #54b0bd;
}
